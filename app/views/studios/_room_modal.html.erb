<div class="modal fade" id="exampleModal-<%=room.id%>" tabindex="-1" aria-labelledby="room-modal-label" aria-hidden="true">
  <div class="modal-dialog modal-xl">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="room-modal-label"><%= room.room_name %></h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="container-fuid">
      <div class="modal-body">
          <%# adding carouhell images %>
          <div class="row">
              <div class="image-carousel col-md-7 col-s-12">
                  <div id="room-control-carousell-<%=room.id%>" class="carousel slide" data-bs-ride="carousel">
                      <div class="carousel-inner">
                          <div class="img-fluid carousel-item active">
                              <img class="d-block w-300 h-200" src="/assets/room3.jpg" alt="First slide">
                          </div>
                          <div class="img-fluid carousel-item">
                              <img class="d-block w-300 h-200" src="/assets/room4.jpg" alt="Second slide">
                          </div>
                          <div class="img-fluid carousel-item">
                              <img class="d-block w-300 h-200" src="/assets/room5.jpg" alt="Third slide">
                          </div>
                      </div>
                    <button class="carousel-control-prev" type="button" data-bs-target="#room-control-carousell-<%=room.id%>" data-bs-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="visually-hidden">Previous</span>
                    </button>
                    <button class="carousel-control-next" type="button" data-bs-target="#room-control-carousell-<%=room.id%>" data-bs-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="visually-hidden">Next</span>
                    </button>
                  </div>
              </div>
              <div class="modal-text col-5">
              <h3><%= room.room_type %>123</h3>
              </div>
          </div>
          <%# end of carohell images top %>
      </div>
      </div>
      <div class="modal-footer">

        <%# <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button> %>
        <%# <button type="button" class="btn btn-primary">Save changes</button> %>
      </div>
    </div>
  </div>
</div>

